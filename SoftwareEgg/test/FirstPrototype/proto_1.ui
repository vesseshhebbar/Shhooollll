<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Toplevel" id="Home_TL">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Labelframe" id="Home_FR">
        <property name="height">200</property>
        <property name="text" translatable="yes">Choose:</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Button" id="LoginHome_BT">
            <property name="text" translatable="yes">Login</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="RegisterHome_BT">
            <property name="text" translatable="yes">Register</property>
            <layout>
              <property name="column">1</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="Login_TL">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="Login_FR">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="UsernameLogin_LB">
            <property name="text" translatable="yes">Enter Username:</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="UsernameLogin_TE">
            <layout>
              <property name="column">1</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="PasswordLogin_LB">
            <property name="text" translatable="yes">Enter Password
</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="UsernamePassword_TE">
            <property name="show">*</property>
            <layout>
              <property name="column">1</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="SubmitLogin_BT">
            <property name="text" translatable="yes">Login</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>

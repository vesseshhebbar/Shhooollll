<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainwindow">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="welcome">
        <property name="anchor">w</property>
        <property name="background">#d8c169</property>
        <property name="borderwidth">0</property>
        <property name="font">TkCaptionFont</property>
        <property name="foreground">#6c0000</property>
        <property name="justify">center</property>
        <property name="padding">30</property>
        <property name="relief">flat</property>
        <property name="text" translatable="yes">Welcome to Password Manager!!</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="text">
        <property name="text" translatable="yes">Entry_1</property>
        <property name="textvariable">string:textEntry</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="button">
        <property name="command">btn_clicked</property>
        <property name="text" translatable="yes">Button_1</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
